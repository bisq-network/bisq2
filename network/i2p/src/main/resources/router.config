# Initial I2P router.config
#
# Memory usage tuning
# Adjusts PRNG buffer settings and bloom filter size. Savings are minor but retained.
#
prng.buffers=2
prng.bufferSize=32768
router.decayingBloomFilterM=20
stat.full=false
#
# Time synchronization
# Disables I2Pâ€™s NTP client. Enable if the system time may be inaccurate.
time.disabled=false
#
# Disable external I2CP interface (port 7654)
#
i2cp.disableInterface=true
#
# Naming service configuration
# Uses hosts.txt for name resolution. SQLite-based service is a future enhancement.
#
i2p.naming.impl=net.i2p.client.naming.HostsTxtNamingService
i2p.hostsfilelist=privatehosts.txt,hosts.txt
#
##### Tunnels
# Tunnel length and quantity configuration for inbound/outbound tunnels
#
router.inboundPool.backupQuantity=0
router.inboundPool.length=2
router.inboundPool.lengthVariance=0
router.inboundPool.quantity=2
router.outboundPool.backupQuantity=0
router.outboundPool.length=2
router.outboundPool.lengthVariance=0
router.outboundPool.quantity=2
router.maxParticipatingTunnels=20
router.sharePercentage=50
#
##### Transport
# Bandwidth limits (in KBps)
#
i2np.bandwidth.inboundKBytesPerSecond=100
i2np.bandwidth.outboundKBytesPerSecond=50
#
# NTCP transport settings
#
i2np.ntcp.enable=true
i2np.ntcp.maxConnections=32
#
# UDP transport settings
#
i2np.udp.enable=true
i2np.udp.maxConnections=32
#
# Enable GeoIP-based location in router console (not applicable on Android)
#
routerconsole.geoip.enable=true
#
# Ensure reseed SSL is enabled (set to false to match expected behavior)
#
router.reseedSSLDisable=false
#
# Enable UPnP to help open firewall ports automatically
#
i2np.upnp.enable=true
#
# Hidden mode disables network participation; not recommended for regular users
#
router.hiddenMode=false
#
# Disable floodfill participation; reduces overhead on the router
#
router.floodfillParticipant=false
